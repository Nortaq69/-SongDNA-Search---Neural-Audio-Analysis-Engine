{
  "name": "song-dna-search",
  "version": "1.0.0",
  "description": "AI-powered audio fingerprinting and song similarity search tool",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run dev-python\" \"electron .\"",
    "dev-python": "cd python && python app.py",
    "start-backend": "cd python && python app.py",
    "start-full": "concurrently \"npm run start-backend\" \"electron .\"",
    "build": "electron-builder",
    "build-python": "cd python && pip install -r requirements.txt",
    "install-all": "npm install && npm run build-python"
  },
  "keywords": ["audio", "fingerprinting", "music", "similarity", "electron"],
  "author": "SongDNA Search",
  "license": "MIT",
  "devDependencies": {
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "socket.io-client": "^4.7.2",
    "wavesurfer.js": "^7.3.0",
    "chart.js": "^4.4.0",
    "three": "^0.158.0",
    "gsap": "^3.12.2"
  },
  "build": {
    "appId": "com.songdna.search",
    "productName": "SongDNA Search",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "assets/**/*",
      "python/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icons/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icons/icon.icns"
    }
  }
}